[
  {
    "ColorConversions": [
      {
        "XPath": "svg/path[2]",
        "Color": "#562c2c"
      },
      {
        "XPath": "svg/path[3]",
        "Color": "#e27171"
      }
    ],
    // Path is relative to json file, not current working directory.
    // Absolute path is also supported.
    "OutputDirectory": "./processed",
    "Input": [
      // Paths are relative to json file, not current working directory.
      // Absolute path is also supported.
      "./sample_icon.svg"
    ],
    "Overwrite": false, 
    "Output": [
      {
        "Path": "android/drawable-hdpi/{name}.png",
        "Width": 24,
        "Height": 24
      },
      {
        "Path": "android/drawable-xhdpi/{name}.png",
        "Width": 32,
        "Height": 32
      },
      {
        "Path": "android/drawable-xxhdpi/{name}.png",
        "Width": 48,
        "Height": 48
      },
      {
        "Path": "android/drawable-xxxhdpi/{name}.png",
        "Width": 60,
        "Height": 60
      },
      {
        "Path": "ios/Assets.x.cassets/{name}.imageset/{name}.png",
        "Width": 16,
        "Height": 16
      },
      {
        "Path": "ios/Assets.x.cassets/{name}.imageset/{name}@2.png",
        "Width": 32,
        "Height": 32
      },
      {
        "Path": "ios/Assets.x.cassets/{name}.imageset/{name}@3.png",
        "Width": 48,
        "Height": 48,
        "ColorConversions": [ // This single icon will be blue instead of red like the others
          {
            "XPath": "svg/path[2]",
            "Color": "#768cdc"
          },
          {
            "XPath": "svg/path[3]",
            "Color": "#2c3556"
          }
        ]
      }
    ]
  },
  {
    "ColorConversions": [
      {
        "XPath": "svg/path[2]",
        "Color": "#e27171"
      },
      {
        "XPath": "svg/path[3]",
        "Color": "#562c2c"
      }
    ],
    // Path is relative to json file, not current working directory.
    // Absolute path is also supported.
    "OutputDirectory": "./processed",
    "Input": [
      // Paths are relative to json file, not current working directory.
      // Absolute path is also supported.
      "./sample_icon.svg"
    ],
    "Overwrite": false,
    "Output": [
      {
        "Path": "android/drawable-night-hdpi/{name}.png",
        "Width": 24,
        "Height": 24
      },
      {
        "Path": "android/drawable-night-xhdpi/{name}.png",
        "Width": 32,
        "Height": 32
      },
      {
        "Path": "android/drawable-night-xxhdpi/{name}.png",
        "Width": 48,
        "Height": 48
      },
      {
        "Path": "android/drawable-night-xxxhdpi/{name}.png",
        "Width": 60,
        "Height": 60
      },
      {
        "Path": "ios/Assets.x.cassets/{name}.imageset/{name}-1.png",
        "Width": 16,
        "Height": 16
      },
      {
        "Path": "ios/Assets.x.cassets/{name}.imageset/{name}@2-1.png",
        "Width": 32,
        "Height": 32
      },
      {
        "Path": "ios/Assets.x.cassets/{name}.imageset/{name}@3-1.png",
        "Width": 48,
        "Height": 48
      }
    ]
  }
]